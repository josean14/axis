@model IEnumerable<AXIS.Models.TruckDetail>


<table class="table">
    <tr>
       
        <th>
            @Html.DisplayNameFor(model => model.LicencePlate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MakeModel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SiteLocation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VIN)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateRent)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
       
        <td>
            @Html.DisplayFor(modelItem => item.LicencePlate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MakeModel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SiteLocation)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VIN)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateRent)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.TruckId, ContractId = @ViewBag.ContractId }) 

        </td>
    </tr>
}

</table>
